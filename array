
#include <iostream>
using namespace std;

int main() {
    float arr[5];

    cout << "Enter elements: ";
    for (int i = 0; i < 5; ++i) {

        cin >> *(arr + i) ;

    }

    cout << "You entered: " << endl;
    for (int i = 0; i < 5; ++i) {

        cout << *(arr + i) << endl ;

    }

    return 0;
}
